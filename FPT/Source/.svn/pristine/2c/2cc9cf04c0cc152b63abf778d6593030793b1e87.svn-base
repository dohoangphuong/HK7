/* main layout
----------------------------------------------------------*/
#banner {
    background-image:url(../IMAGES/blue-banner.jpg);
    background-repeat:no-repeat;
    background-size:cover;
    height:55px;
}

    #banner .member-box {
        float:right;
        padding-top:12px;
        padding-right:10px;
    }

        .member-box input {
            margin-left:5px;
            height:25px !important;
        }

        .member-box .remember-me {
            height:auto !important;
        }

        .member-box a {
            color:#fff;
        }

    #banner #logo {
    margin-top:5px;
    margin-left:65px;
}

#left-menu-background {
    border-right:groove;
    position: fixed;
    width:17%;
    overflow: visible;
    top: 55px;
    bottom: 0;
}

#left-menu {
}

    #left-menu h4 {
        color:blue;
    }

    #left-menu #menu-function {
    }

        #menu-function .top-ul{
            color:gray;
            font-weight:600;
            list-style-type:none;
            padding-left:0px;
        }

            .top-ul li {
                margin-bottom:10px;
            }

            .top-ul .child-ul {
                color:gray;
                font-weight:normal;
                list-style-type: disc;
            }

                .child-ul li{
                    margin-bottom:auto;
                }

    #left-menu a {
        color:gray;
    }
#content {
}

/* page elements
----------------------------------------------------------*/
/* text decorate */
.blue-text {
    color:blue;
}

.gray-text {
    color:gray;
}

/*List with no bullet*/
.no-bullet {
    list-style-type:none;
}

/*Submit button look like a link*/
.submitLink {
  background-color: transparent;
  text-decoration: underline;
  border: none;
  color: gray;
  cursor: pointer;
}

submitLink:focus {
  outline: none;
}

/* fill form */
.fill-form {
}

    .fill-form label{
        width:15%;
    }

    .fill-form input[type=text]{
        width:25%;
    }

    .fill-form input[type=password]{
        width:25%;
    }

#logoutForm {
    display: inline;
}

/* static list */
.label-stt {
    width:15%;
}

.label-count {
    width:5%;
}

/* form layout */
.width20 {
    width:20%;
}

.width40 {
    width:40%;
}

.width75 {
    width:75%;
}

legend {
    display: block;
    width: auto;
    padding: 0 5px;
    margin-bottom: 0;
    font-size: inherit;
    font-weight:bold;
    line-height: inherit;
    border:thin;
    border-bottom: none;
    color:gray;
}

fieldset {
    border: 2px groove threedface;
    margin-bottom:5px;
}

/* static table */
.static-table {
    width:200px;
    margin-left:auto;
    margin-right:auto;
}

/* action panel */
.action-panel {
    bottom:0;
    float:right;
}

/*Nav tab*/
.tab-content {
    border-left: 1px solid #ddd;
    border-right: 1px solid #ddd;
    border-bottom:1px solid #ddd;
    padding: 10px;
    min-height:250px;
    max-height:250px;
    overflow:scroll;
}

    .tab-content label {
        font-weight:normal !important;
    }

.nav-tabs {
    margin-bottom: 0;
}

.btn-like {
    display: inline-block;
    padding: 6px 12px;
    margin-bottom: 0;
    font-weight:bold;
    line-height: 1.42857143;
    text-align: center;
    white-space: nowrap;
    vertical-align: middle;
    background-image: none;
    border: 1px solid transparent;
    border-radius: 4px;
    background-color: #fff;
    border-color: #ccc;
}

/* info and errors */
.message-info {
    border: 1px solid;
    clear: both;
    padding: 10px 20px;
}

.message-error {
    clear: both;
    color: #e80c4d;
    font-size: 1.1em;
    font-weight: bold;
    margin: 20px 0 10px 0;
}

.message-success {
    color: #7ac0da;
    font-size: 1.3em;
    font-weight: bold;
    margin: 20px 0 10px 0;
}

.error {
    color: #e80c4d;
}

.alert-block {
    padding-top: 10px;
    padding-bottom: 10px;
    color: #b94a48;
    background-color: #f2dede;
    border-color: #eed3d7;
}

/* styles for validation helpers */
.field-validation-error {
    color: #e80c4d;
    font-weight: bold;
}

.field-validation-valid {
    display: none;
}

input.input-validation-error {
    border: 1px solid #e80c4d;
}

input[type="checkbox"].input-validation-error {
    border: 0 none;
}

.validation-summary-errors {
    color: #e80c4d;
    font-weight: bold;
    font-size: 1.1em;
}

.validation-summary-valid {
    display: none;
}
